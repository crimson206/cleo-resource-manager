{
  "auth": {
    "github": {
      "method": "auto"
    }
  },
  "providers": {
    "github": [
      {
        "name": "example-github",
        "enabled": true,
        "url": "https://github.com/crimson206/my-prompts",
        "default_branch": "main",
        "resource_dir": "prompts",
        "target_dir": ".github_resources",
        "timeout": 10
      }
    ],
    "local": [
      {
        "name": "example-local",
        "enabled": true,
        "path": "./local-resources",
        "target_dir": ".local_resources"
      }
    ]
  },
  "resources": {
    "include_patterns": [
      "*.txt",
      "*.md"
    ],
    "exclude_patterns": [
      ".git",
      "__pycache__",
      "*.pyc"
    ]
  }
}